            <Original>Default</Original>
            <English>Default</English>
            <Korean>기본</Korean>
        </Key>
        <Key ID="STR_tts_emission_true">
            <Original>True</Original>
            <English>True</English>
            <Korean>활성화</Korean>
        </Key>
        <Key ID="STR_tts_emission_false">
            <Original>False</Original>
            <English>False</English>
            <Korean>비활성화</Korean>
        </Key>
